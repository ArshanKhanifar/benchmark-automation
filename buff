
		"make -C /usr/src/tools/tools/syscall_timing",
		"make install -C /usr/src/tools/tools/syscall_timing",
		"/usr/obj/usr/src/amd64.amd64/tools/tools/syscall_timing/syscall_timing getppid > output/getppid.log",

